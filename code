#!/bin/bash

clear
pyfiglet starting
sleep 2
clear
pyfiglet -f bloody -c red Reaper


# Function to send the image link to the Discord webhook
send_image_link_to_webhook() {
    curl -X POST -H "Content-Type: application/json" -d "{\"content\": \"Reaper image lo>
}

# Prompt the user for input
echo "Enter the image URL:"
read image_url
echo "Enter the Discord webhook URL:"
read webhook_url

# Send the image link to the Discord webhook
send_image_link_to_webhook "$image_url" "$webhook_url"
sleep 1
clear
pyfiglet -f bloody -c red reaper
echo "Made by Midnight"
echo "-----------------"
echo "Discord mtvvon"
echo "--------------"
echo "Your image logger was succesfully made, you can see the image logger in your serve>
sleep 1

